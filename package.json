{
  "name": "@wethegit/react-hooks",
  "version": "1.0.1",
  "description": "A collection of helpers for use in React projects.",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/react-hooks.umd.cjs",
  "module": "./dist/react-hooks.js",
  "exports": {
    ".": {
      "import": "./dist/react-hooks.js",
      "require": "./dist/react-hooks.umd.cjs"
    }
  },
  "scripts": {
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "dev": "vite",
    "build": "npm run lint:scripts && vite build",
    "lint:scripts": "eslint --fix --ext .jsx,js --ignore-path .gitignore ."
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "keywords": [
    "react",
    "hooks",
    "helpers",
    "local storage",
    "accessibility"
  ],
  "homepage": "https://github.com/wethegit/react-hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/wethegit/react-hooks"
  },
  "bugs": {
    "url": "https://github.com/wethegit/react-hooks/issues"
  },
  "author": "Andrew Rubin <andrew@wethecollective.com> (https://wethecollective.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "vite": "^4.3.2"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
